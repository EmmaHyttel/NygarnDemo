@page
@using NygarnDemo.Models;
@model NygarnDemo.Pages.Product.ToolPages.DeleteToolModel
@{
}

<h1>Delete Confirmation></h1>
<div class="alert alert-danger">
	<h5>Are you sure you want to delete tool - @Model.Tool.Name</h5>
	<form method="post">
		<button type="submit" class="btn btn-danger">Yes</button>
		<a class="btn btn-primary" asp-page="/ToolPages/GetAllTools">No</a>
	</form>
</div>